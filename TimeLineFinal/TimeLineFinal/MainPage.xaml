<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             xmlns:local="clr-namespace:TimeLine"
             x:Class="TimeLineFinal.MainPage"
             Title="TimeLine">

    <StackLayout>
        <Label x:Name="lbMessage" IsVisible="False" Text=""/>
        <Entry x:Name="enLogin" Placeholder="Login"/>
        <Entry x:Name="enPass" IsPassword="True" Placeholder="Senha"/>
        <Button Text="Entrar" Clicked="Entrar_Clicked"/>
        <Button Text="Não tenho uma conta" Clicked="Novo_Clicked"/>
    </StackLayout>

</ContentPage>
