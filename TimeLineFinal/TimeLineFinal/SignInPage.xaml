<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="TimeLineFinal.SignInPage"
             Title="TimeLine">
    <ContentPage.Content>
        <StackLayout>
            <Label x:Name="lbMessage"/>
            <Entry x:Name="enEmail" Placeholder="Email"/>
            <Entry x:Name="enLogin" Placeholder="Login"/>
            <Entry x:Name="enPass" IsPassword="True" Placeholder="Senha"/>
            <Button Text="Registrar" Clicked="Registrar_Clicked"/>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>